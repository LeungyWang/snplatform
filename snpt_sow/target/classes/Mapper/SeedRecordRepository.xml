<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wly.repository.SeedRecordRepository">

    <select id="findAll" resultType="SeedRecord">
        select * from seedrecord limit #{param1},#{param2}
    </select>

    <select id="countById" resultType="int">
        select count(id) from seedrecord where id = #{id}
    </select>

    <select id="count" resultType="int">
        select count(id) from seedrecord
    </select>

</mapper>